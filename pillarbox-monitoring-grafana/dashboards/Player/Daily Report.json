        {
          "id": "reduce",
          "options": {
            "includeTimeField": false,
            "labelsToFields": false,
            "mode": "seriesToRows",
            "reducers": [
              "sum"
            ]
          }
        },
        {
          "id": "rowsToFields",
          "options": {}
        },
        {
          "id": "calculateField",
          "options": {
